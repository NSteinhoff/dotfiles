{
  "$schema": "https://opencode.ai/config.json",
  "theme": "system",
  "provider": {
    "lm-studio": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "LM Studio (local)",
      "options": {
        "baseURL": "http://127.0.0.1:1234/v1"
      },
      "models": {
        "qwen/qwen3-coder-30b": {
          "name": "qwen-coder",
          "tool_call": true
        }
      }
    }
  },
  "keybinds": {
    "leader": "ctrl+x",
    "session_child_cycle": "<leader>]",
    "session_child_cycle_reverse": "<leader>[",
  }
}
