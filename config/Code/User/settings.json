{
    "breadcrumbs.enabled": true,
    "keyboard.dispatch": "keyCode",
    "liveshare.sharedTerminalHeight": 120,
    "peacock.vslsJoinColor": "#f9e64f",
    "vim.useSystemClipboard": true,
    "window.menuBarVisibility": "toggle",
    "files.associations": {
        "vimrc": "viml",
        "gvimrc": "viml",
        "*.vim": "viml",
    },
    "editor.minimap.enabled": false,
    "vim.handleKeys": {
        "<C-j>": false,
        "<C-k>": false,
        "<C-f>": false,
        "<C-b>": false
    },
    "editor.fontSize": 16
}
